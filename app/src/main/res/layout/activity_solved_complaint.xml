<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Solved_complaint">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_phone_solved"
        android:layout_width="412dp"
        android:layout_height="314dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toTopOf="@+id/rv_vehicle_solved"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/missingcomplaint_show" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_vehicle_solved"
        android:layout_width="414dp"
        android:layout_height="333dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rv_phone_solved"
        tools:listitem="@layout/missing_vehicle_show" />
</androidx.constraintlayout.widget.ConstraintLayout>